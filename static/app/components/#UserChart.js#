Ext.define('Kitchensink.components.UserChart', {
    extend: 'Ext.Component',
    xtype:'userChart',
    templates:[
        {tag : 'canvas',
		 reference: 'starchart',
		 cls: 'starChart',
         width:1024,
         height:718
	    }
    ],
    initialize : function(){
        this.callParent();
        
        this.element.on({
            scope: this,
            painted : 'paint',
            tap:'onTap'
        });
    },
    
});